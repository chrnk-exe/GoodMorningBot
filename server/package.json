{
  "name": "server",
  "version": "1.0.0",
  "description": "nodejs server for bot",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "startjs": "npm run build && npm run start-server",
    "startts": "cd src && nodemon --files index.ts",
    "start-server": "cd server && node index.js",
    "build": "cd src && npx tsc",
    "migrate": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "unseeds": "npx sequelize-cli db:seed:undo:all",
    "unmigrate": "npm run unseeds && npx sequelize-cli db:migrate:undo:all",
    "re-migrate": "npm run build && npm run unmigrate && npm run migrate",
    "startpm2": "npm run build && cd server && pm2 start index.js"
  },
  "author": "zxchrnk",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.8.0",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.14",
    "@types/nodemailer": "^6.4.6",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
