{
  "name": "server",
  "version": "1.0.0",
  "description": "nodejs server for bot",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && npm run start-server",
    "start-server": "cd server && node index.js",
    "build": "cd src && npx tsc",
    "migrate": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "unseeds": "npx sequelize-cli db:seed:undo:all",
    "unmigrate": "npx sequelize-cli db:migrate:undo",
    "re-migrate": "npm run unmigrate && npm run migrate",
    "startpm2": "npm run build && cd server && pm2 start index.js"
  },
  "author": "zxchrnk",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.14",
    "@types/pg": "^8.6.5",
    "dotenv": "^16.0.2",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.7.4"
  }
}
